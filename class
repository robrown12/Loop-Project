public static void main (String[] args) {
}
// Loop that will display a string 99 times
for (int i = 0; i < 99; i++) {
    System.out.println(" This is string number:" + (i + 1));
}
// Loop that will display every odd number from 0 to 99
for (int i = 1; i <= 99; i += 2) {
    System.out.println(i);
}
// Loop that will display every even number from 0 to 99
for (int i = 0; i <= 99; i += 2) {
    System.out.println(i);
}
// Loop that will display every even number from 0 to 99
for (int i = 0; i <= 99; i++) {
    if (i % 2 == 0) {
        System.out.println(i);
    }
}
// Loop that will sum up the odd numbers from 0 to 99 and print at the end.
int oddSum = 0;
for (int i = 1; i <= 99; i += 2) {
    oddSum += i;
}
// Loop that will sum up the even numbers from 0 to 99 and print it at the end.
System.out.println("Sum of odd numbers from 0 to 99 is:" + oddSum);
int evenSum = 0;
for (int i = 0; i <= 99; i += 2) {
    evenSum += i;
}
// Loop that finds a sum of the numbers between two numbers that are given by the user, inclusive.
System.out.println("Sum of the ven number from 0 to 99:" + evenSum")
